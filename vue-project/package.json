{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "watch": "vite build --mode development --base /static/dev/ --outDir ../static/dev/ -w",
    "build": "vite build --mode production --base /static/prod/ --outDir ../static/prod/",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bootstrap": "5.2.0",
    "echarts": "^5.4.3",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-datepicker-next": "^1.0.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.0.0",
    "sass": "1.54.0",
    "vite": "^4.4.6"
  }
}
